services:
  humanrelations.api:
    build:
      context: C:\Vakken\Fullstack .NET\Labs\KWops\src
      dockerfile: Services/HumanRelations/HumanRelations.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: HumanRelations.API
      target: base
    container_name: HumanRelations.API
    depends_on:
      sqldata:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionString: Data Source=sqldata;Database=KWops.HumanRelations;UserID=sa;Password=Pass@word
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: humanrelationsapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/HumanRelations.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - published: 5001
      target: 443
    tty: true
    volumes:
    - C:\Vakken\Fullstack .NET\Labs\KWops\src\Services\HumanRelations\HumanRelations.API:/app:rw
    - C:\Users\11901860\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\11901860\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\11901860\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\11901860\.nuget\packages:/root/.nuget/packages:ro
    - C:\Vakken\Fullstack .NET\Labs\KWops\src:/src:rw
  sqldata:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Pass@word
    image: mrc.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
    volumes:
    - kwops-sqldata:/var/opt/mssql:rw
version: '3.4'
volumes:
  kwops-sqldata:
    external: false